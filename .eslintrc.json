{
    "env": {
        "browser": true,
        "node": true,
        "worker": true,
        "es2022": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "array-callback-return": [
            "error"
        ],
        "no-await-in-loop": [
            "error"
        ],
        "no-constant-binary-expression": [
            "error"
        ],
        "no-constructor-return": [
            "error"
        ],
        "no-duplicate-imports": [
            "error",
            {
                "includeExports": true
            }
        ],
        "no-new-native-nonconstructor": [
            "error"
        ]
    },
    "noInlineConfig": true // 禁用内嵌型注释
}