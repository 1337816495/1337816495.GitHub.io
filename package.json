{
	"name": "www.jynxio.com",
	"private": true,
	"version": "always the latest",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"deploy": "vercel deploy --prod dist",
		"preview": "vite preview",
		"prettier": "prettier . --write",
		"eslint": "eslint --ext .js,.ts,.jsx,.tsx --ignore-path .gitignore src --fix",
		"stylelint": "stylelint \"**/*.{css,scss,sass,less,postcss}\" --fix",
		"build:all": "pnpm run build:catalog && pnpm run build:blog && pnpm run build:webp",
		"build:font": "node --loader ts-node/esm ./src/generators/createFont.ts",
		"build:blog": "node ./src/generators/createBlog.js",
		"build:webp": "node --loader ts-node/esm ./src/generators/createWebp.ts",
		"build:catalog": "node --loader ts-node/esm ./src/generators/createCatalog.ts"
	},
	"dependencies": {
		"@solidjs/router": "^0.8.4",
		"@types/three": "^0.157.2",
		"fuse.js": "^6.6.2",
		"solid-js": "^1.8.7",
		"three": "^0.159.0"
	},
	"devDependencies": {
		"@konghayao/cn-font-split": "^4.10.6",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.10.3",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^5.0.1",
		"fs-extra": "^11.2.0",
		"hast-util-to-html": "^8.0.4",
		"mdast-util-from-markdown": "^2.0.0",
		"mdast-util-gfm": "^3.0.0",
		"mdast-util-to-hast": "^13.0.2",
		"micromark-extension-gfm": "^3.0.0",
		"nanoid": "^4.0.2",
		"postcss": "^8.4.32",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^9.3.0",
		"postcss-sorting": "^8.0.2",
		"prettier": "^3.1.0",
		"sharp": "^0.32.6",
		"shiki": "^0.14.5",
		"stylelint": "^15.11.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-standard": "^34.0.0",
		"svgson": "^5.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.2",
		"vercel": "^32.6.1",
		"vite": "^5.0.6",
		"vite-plugin-solid": "^2.7.2"
	}
}
