{
	"name": "www.jynxio.com",
	"private": true,
	"version": "always the latest",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"deploy": "vercel deploy --prod dist",
		"fix": "npm run lint:js && npm run lint:css && npm run format",
		"format": "prettier --write .",
		"lint:js": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:css": "stylelint \"**/*.{css,scss,sass,less,postcss}\" --fix",
		"build:all": "pnpm run build:catalog && pnpm run build:blog && pnpm run build:webp",
		"build:font": "node --loader ts-node/esm ./src/generators/createFont.ts",
		"build:blog": "node ./src/generators/createBlog.js",
		"build:webp": "node --loader ts-node/esm ./src/generators/createWebp.ts",
		"build:catalog": "node --loader ts-node/esm ./src/generators/createCatalog.ts"
	},
	"dependencies": {
		"@solidjs/router": "^0.10.5",
		"fuse.js": "^7.0.0",
		"solid-js": "^1.8.7",
		"three": "^0.160.0"
	},
	"devDependencies": {
		"@konghayao/cn-font-split": "^4.11.0",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.10.5",
		"@types/three": "^0.160.0",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-solid": "^0.13.0",
		"fs-extra": "^11.2.0",
		"hast-util-to-html": "^9.0.0",
		"mdast-util-from-markdown": "^2.0.0",
		"mdast-util-gfm": "^3.0.0",
		"mdast-util-to-hast": "^13.0.2",
		"micromark-extension-gfm": "^3.0.0",
		"nanoid": "^5.0.4",
		"postcss": "^8.4.32",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^9.3.0",
		"prettier": "^3.1.1",
		"sharp": "^0.33.1",
		"shiki": "^0.14.7",
		"stylelint": "^16.1.0",
		"stylelint-config-standard": "^36.0.0",
		"svgson": "^5.3.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vercel": "^33.0.1",
		"vite": "^5.0.10",
		"vite-plugin-solid": "^2.8.0"
	}
}
