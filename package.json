{
	"name": "www.jynxio.com",
	"private": true,
	"version": "always the latest",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"deploy": "vercel deploy --prod dist",
		"lint:js": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:css": "stylelint \"**/*.{css,scss,sass,less,postcss}\" --fix",
		"lint:style": "prettier --write .",
		"lint:all": "npm run lint:js && npm run lint:css && npm run lint:style",
		"build:all": "pnpm run build:catalog && pnpm run build:blog && pnpm run build:webp",
		"build:font": "node --loader ts-node/esm ./src/generators/createFont.ts",
		"build:blog": "node ./src/generators/createBlog.js",
		"build:webp": "node --loader ts-node/esm ./src/generators/createWebp.ts",
		"build:catalog": "node --loader ts-node/esm ./src/generators/createCatalog.ts",
		"prepare": "husky",
		"lintstaged": "lint-staged",
		"commitlint": "commitlint --edit"
	},
	"dependencies": {
		"@solidjs/router": "^0.10.10",
		"fuse.js": "^7.0.0",
		"solid-js": "^1.8.12",
		"three": "^0.161.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.0",
		"@commitlint/config-conventional": "^18.6.0",
		"@konghayao/cn-font-split": "^4.11.2",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.11.13",
		"@types/three": "^0.160.0",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-solid": "^0.13.1",
		"fs-extra": "^11.2.0",
		"hast-util-to-html": "^9.0.0",
		"husky": "^9.0.7",
		"lint-staged": "^15.2.1",
		"mdast-util-from-markdown": "^2.0.0",
		"mdast-util-gfm": "^3.0.0",
		"mdast-util-to-hast": "^13.1.0",
		"micromark-extension-gfm": "^3.0.0",
		"nanoid": "^5.0.4",
		"postcss": "^8.4.33",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^9.3.0",
		"prettier": "^3.2.4",
		"sharp": "^0.33.2",
		"shiki": "1.0.0-beta.1",
		"stylelint": "^16.2.0",
		"stylelint-config-standard": "^36.0.0",
		"svgson": "^5.3.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vercel": "^33.4.1",
		"vite": "^5.0.12",
		"vite-plugin-solid": "^2.9.1"
	}
}
