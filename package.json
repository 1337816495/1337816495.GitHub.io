{
    "name": "www.jynxio.com",
    "private": true,
    "version": "always the latest",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "deploy": "vercel deploy --prod dist",
        "preview": "vite preview",
        "build:all": "pnpm run build:catalog && pnpm run build:blog && pnpm run build:webp",
        "build:font": "node --loader ts-node/esm ./src/utils/createFont.ts",
        "build:blog": "node ./src/utils/createBlog.js",
        "build:webp": "node --loader ts-node/esm ./src/utils/createWebp.ts",
        "build:catalog": "node --loader ts-node/esm ./src/utils/createCatalog.ts"
    },
    "dependencies": {
        "@solidjs/router": "^0.8.3",
        "fuse.js": "^6.6.2",
        "solid-js": "^1.8.3"
    },
    "devDependencies": {
        "@konghayao/cn-font-split": "^4.9.0",
        "@types/fs-extra": "^11.0.3",
        "@types/node": "^20.8.7",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-prettier": "^5.0.1",
        "fs-extra": "^11.1.1",
        "hast-util-to-html": "^8.0.4",
        "mdast-util-from-markdown": "^2.0.0",
        "mdast-util-gfm": "^3.0.0",
        "mdast-util-to-hast": "^13.0.2",
        "micromark-extension-gfm": "^3.0.0",
        "nanoid": "^4.0.2",
        "postcss": "^8.4.31",
        "postcss-nested": "^6.0.1",
        "postcss-preset-env": "^9.2.0",
        "postcss-sorting": "^8.0.2",
        "prettier": "^3.0.3",
        "sharp": "^0.32.6",
        "shiki": "^0.14.5",
        "stylelint": "^15.11.0",
        "stylelint-config-standard": "^34.0.0",
        "svgson": "^5.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "vercel": "^32.4.1",
        "vite": "^4.5.0",
        "vite-plugin-solid": "^2.7.2"
    }
}
